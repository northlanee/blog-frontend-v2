{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "rewriteImportExtensions": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "targets": "> 0.25%, not dead"
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-class-properties"]
}
